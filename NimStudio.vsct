                <Icon guid="guidImages" id="bmpPic1" />
                <Strings>
                    <ButtonText>cmdid_g1c2</ButtonText>
                </Strings>
            </Button>
            <Button guid="CmdSet1GUID" id="cmdid_g1c3" priority="0x0100" type="Button">
                <Parent guid="CmdSet1GUID" id="MenuGroup2" />
                <Icon guid="guidImages" id="bmpPic1" />
                <Strings>
                    <ButtonText>cmdid_g1c3</ButtonText>
                </Strings>
            </Button>
